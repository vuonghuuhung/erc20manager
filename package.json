{
  "name": "erc20manger",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev:node": "turbo run dev:node",
    "dev:app": "turbo run dev:app",
    "dev:mock": "turbo run dev:mock",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "add-components": "turbo run add-components --",
    "compile": "turbo run compile --filter=@repo/contracts",
    "test:forge": "turbo run test:forge",
    "compile:forge": "turbo run compile:forge"
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "turbo": "^2.4.0",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.12"
  }
}
